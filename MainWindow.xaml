<Window x:Class="Parcial1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parcial1"
        mc:Ignorable="d"
        Title="Registro" Height="250" Width="390">
    <Grid>

        <StackPanel> 
            <Menu> 
                <MenuItem Header="Registros" >
                    <MenuItem Header="Registro" x:Name="rRegistro" Click ="RegistroMenuItem_Click" /> 
                    </MenuItem>

                <MenuItem Header="Consultas" >
                    <MenuItem Header="Consulta" x:Name="cConsulta" Click ="ConsultaMenuItem_Click" />  
                    </MenuItem>
            </Menu>

        </StackPanel>

    </Grid>
</Window>